<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns="sap.m">
	<TableSelectDialog  id="idAddBudgetPaymentFormTable" inset="false" noDataText="No Budget Approval Forum Found" title="Budget Approval Form" 
search="handleSearch" confirm="handleClose" cancel="handleClose" items="{ path: '/' }">
		<ColumnListItem>
			<cells>
				<Text text="{BAFNO}"/>
				<Text text="{DEPPR}"/>
				<Text text="{TITLE}"/>
				<Text text="{RQDAT}"/>
				<Text text="{BPSTA}"/>
				<Text text="{BPEND}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<header>
					<Text text="Budget Number"/>
				</header>
			</Column>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<header>
					<Text text="Department"/>
				</header>
			</Column>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<header>
					<Text text="Title"/>
				</header>
			</Column>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<header>
					<Text text="Created On"/>
				</header>
			</Column>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<header>
					<Text text="Budget Period Start"/>
				</header>
			</Column>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<header>
					<Text text="Budget Period End"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>